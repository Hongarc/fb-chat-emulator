{
  "name": "node-facebook",
  "version": "0.5.8",
  "description": "Facebook simulator web browser",
  "author": "Hongarc",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hongarc/node-facebook.git"
  },
  "main": "dist/index.js",
  "dependencies": {
    "kiat-html-parser": "^1.2.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.6",
    "@types/request": "2.48.1",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "chai": "4.2.0",
    "codecov": "3.3.0",
    "del": "^4.1.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-import": "2.17.1",
    "gulp": "^4.0.0",
    "gulp-typescript": "^5.0.1",
    "gulp-uglify": "^3.0.2",
    "mocha": "6.1.3",
    "nyc": "13.3.0",
    "rimraf": "2.6.3",
    "ts-node": "8.1.0",
    "typescript": "3.4.3"
  },
  "scripts": {
    "test": "nyc mocha test/**/*.spec.ts --timeout 40000",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "npm run lint:src && npm run lint:spec",
    "lint:src": "eslint src/**/*.ts --fix",
    "lint:spec": "eslint test/**/*.ts --fix",
    "build": "gulp",
    "start": "tsc -watch"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "include": [
      "src/**"
    ],
    "reporter": [
      "html"
    ],
    "extension": [
      ".ts"
    ]
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "nodejs",
    "typescript",
    "facebook",
    "api",
    "bot",
    "chat",
    "node-facebook",
    "request",
    "cookie",
    "gulp"
  ]
}
